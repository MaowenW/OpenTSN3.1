<?xml version="1.0" encoding="utf-8"?>
<network_init_cfg>   
<!-- 控制器的IMAC地址默认为0-->
<tsnlight_info>
	<master_imac>2</master_imac>   <!-- master_imac表示主时钟的imac地址为1-->
	<sync_period>200</sync_period>    <!-- sync_period表示时间同步的周期为200ms--> 
	<sync_flowID>4096</sync_flowID>    <!--sync_flowID表示sync组播的flowID为4096-->
	<report_period>200</report_period>  <!-- report_period表示上报周期为200ms-->
</tsnlight_info>	

<switch> 
	<register> <!-- TSN交换机的属性信息-->
		<IMAC>2</IMAC>   <!-- 交换机的IMAC地址-->
		<port_type>0</port_type>    <!-- 示例port_type=253（二进制8b’1111 1101）表示设备的7、6、5、4、3、2、0号端口为非协作模式，1号端口为协作模式--> 
		<time_slot>512</time_slot>     <!--时间槽的长度为32us-->
		<inject_slot_period>4</inject_slot_period>  <!-- 注入周期为4-->
		<submit_slot_period>4</submit_slot_period>  <!-- 提交周期为4-->
		<qbv_or_qch>0</qbv_or_qch>                    <!-- 调度模型为qbv-->
		<ST_buf_threshold>200</ST_buf_threshold>   <!-- ST帧的缓冲区块阈值-->
	</register> <!-- 交换机的属性信息-->
	<forward_table>  <!-- 交换机的初始转发表-->         
		<entry> 
			<flowid>0</flowid> 
			<outport>4</outport>  
		</entry> 	
		<entry> <!-- 交换机0 -->
			<flowid>2</flowid> 
			<outport>256</outport>  
		</entry> 	
		<entry> <!-- 交换机1 -->
			<flowid>3</flowid> 
			<outport>1</outport>  
		</entry> 
		<entry> <!-- 交换机2 -->
			<flowid>4</flowid> 
			<outport>1</outport>  
		</entry> 		
		<entry> 
			<flowid>4096</flowid> 
			<outport>1</outport>  
		</entry> 	
	</forward_table>
</switch>

<switch> 
	<register> <!-- TSN交换机0的单个寄存器信息-->
		<IMAC>3</IMAC>   <!-- TSN交换机1的IMAC地址-->
		<port_type>0</port_type>    <!-- 端口类型，port_type使用bitmap的形式，8bit，从高到低依次代表7、6、5、4、3、2、1、0号端口。0代表协作模式，仅能接收映射报文，1代表非协作模式，仅能接收未映射报文。示例port_type=252（二进制8b’1111 1100）表示设备的7、6、5、4、3、2号端口为非协作模式，1、0号端口为协作模式--> 
		<time_slot>512</time_slot>     <!--时间槽的长度为32us-->
		<inject_slot_period>4</inject_slot_period>  <!-- 注入周期为4-->
		<submit_slot_period>4</submit_slot_period>  <!-- 提交周期为4-->
		<qbv_or_qch>0</qbv_or_qch>                    <!-- 调度模型为qbv-->
		<ST_buf_threshold>200</ST_buf_threshold>   <!-- ST帧的缓冲区块阈值-->
	</register> 

	<forward_table>  <!-- 交换机0的初始转发表-->         
		<entry> 
			<flowid>0</flowid> 
			<outport>2</outport>  
		</entry> 	
		<entry> <!-- 交换机0 -->
			<flowid>2</flowid> 
			<outport>2</outport>  
		</entry> 	
		<entry> <!-- 交换机1 -->
			<flowid>3</flowid> 
			<outport>256</outport>  
		</entry> 
		<entry> <!-- 交换机2 -->
			<flowid>4</flowid> 
			<outport>1</outport>  
		</entry> 		
		<entry> 
			<flowid>4096</flowid> 
			<outport>257</outport>  
		</entry> 
	</forward_table>
</switch>

<switch> 
	<register> <!-- TSN交换机1的单个寄存器信息-->
		<IMAC>4</IMAC>   <!-- TSN交换机1的IMAC地址-->
		<port_type>0</port_type>    <!-- 端口类型，port_type使用bitmap的形式，8bit，从高到低依次代表7、6、5、4、3、2、1、0号端口。0代表协作模式，仅能接收映射报文，1代表非协作模式，仅能接收未映射报文。示例port_type=252（二进制8b’1111 1100）表示设备的7、6、5、4、3、2号端口为非协作模式，1、0号端口为协作模式--> 
		<time_slot>512</time_slot>     <!--时间槽的长度为32us-->
		<inject_slot_period>4</inject_slot_period>  <!-- 注入周期为4-->
		<submit_slot_period>4</submit_slot_period>  <!-- 提交周期为4-->
		<qbv_or_qch>0</qbv_or_qch>                    <!-- 调度模型为qbv-->
		<ST_buf_threshold>200</ST_buf_threshold>   <!-- ST帧的缓冲区块阈值-->
	</register> 

	<forward_table>  <!-- 交换机1的初始转发表-->         
		<entry> 
			<flowid>0</flowid> 
			<outport>2</outport>  
		</entry> 	
		<entry> <!-- 交换机0 -->
			<flowid>2</flowid> 
			<outport>2</outport>  
		</entry> 	
		<entry> <!-- 交换机1 -->
			<flowid>3</flowid> 
			<outport>2</outport>  
		</entry> 
		<entry> <!-- 交换机2 -->
			<flowid>4</flowid> 
			<outport>256</outport>  
		</entry> 		
		<entry> 
			<flowid>4096</flowid> 
			<outport>256</outport>  
		</entry> 		
	</forward_table>
</switch>



</network_init_cfg>


